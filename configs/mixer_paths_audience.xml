	<!-- main_mic : Left -->
	<!-- 2nd_mic : Right -->
	<!-- headset_mic : Right -->
	<!-- 3rd_mic : Right -->
	<!-- sco : None -->
	<path name="capture_channel_left">
		<ctl name="AIF1TX1 Input 1" value="LHPF1" />
		<ctl name="AIF1TX2 Input 1" value="LHPF1" />
	</path>

	<path name="capture_channel_right">
		<ctl name="AIF1TX1 Input 1" value="LHPF2" />
