	<ctl name="DAC COMP 2" value="1 1" />
	<ctl name="FRF COEFF 1L" value="3 244 247 48 6 4 254 205" />
	<ctl name="FRF COEFF 1R" value="3 244 247 48 6 4 254 205" />

