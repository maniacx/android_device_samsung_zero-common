	<ctl name="ASRC1IN2R Input" value="AIF2RX2" />

	<ctl name="ISRC2 FSL" value="SYNCCLK rate 2" />
	<ctl name="ISRC2 FSH" value="SYNCCLK rate 1" />
	<ctl name="ISRC2DEC1 Input" value="IN1L" />
