	<ctl name="ASRC2 Rate 1" value="SYNCCLK rate 1" />
	<ctl name="ASRC2 Rate 2" value="ASYNCCLK rate 2" />

	<ctl name="HPOUT1L Input 1" value="AIF1RX1" />
	<ctl name="HPOUT1R Input 1" value="AIF1RX2" />
