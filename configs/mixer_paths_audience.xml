	<ctl name="Sample Rate 2" value="16kHz" />
	<ctl name="DSP2 Rate" value="SYNCCLK rate 3" />
	<ctl name="DSP3 Rate" value="SYNCCLK rate 3" />
	<ctl name="DSP4 Rate" value="SYNCCLK rate 2" />

	<ctl name="ASRC1 Rate 1" value="SYNCCLK rate 1" />
