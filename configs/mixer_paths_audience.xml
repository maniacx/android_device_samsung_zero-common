		<ctl name="HPOUT1R Impedance Volume" value="114" />
	</path>

	<!-- Output stage -->
	<!-- media playback -->
	<path name="media-handset">
		<ctl name="CP Mode" value="Inverting" />
		<ctl name="HPOUT3L Input 1" value="AIF1RX1" />
		<ctl name="HPOUT3R Input 1" value="AIF1RX2" />
